{
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/node_modules/**": true,
      ".tmp": true
    },
    "files.exclude": {
      ".tmp": true
    },
    "search.exclude": {
      ".tmp": true
    },
    "json.schemas": [
      {
        "fileMatch": [
          "pbiviz.json"
        ],
        "url": "./node_modules/powerbi-visuals-api/schema.pbiviz.json"
      },
      {
        "fileMatch": [
          "capabilities.json"
        ],
        "url": "./node_modules/powerbi-visuals-api/schema.capabilities.json"
      },
      {
        "fileMatch": [
          "dependencies.json"
        ],
        "url": "./node_modules/powerbi-visuals-api/schema.dependencies.json"
      }
    ]
  }
